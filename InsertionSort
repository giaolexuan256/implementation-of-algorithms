#include <iostream>

using namespace std;

void printArray(int a[], int length) {
    for (int i = 0; i < length; i++) {
        cout << a[i] << " ";
    }

    cout << endl;
}

void insertionSort(int a[], int length) {
    for (int j = 1; j < length; j++) {
        int key = a[j];
        int i = j - 1;
        while (i >= 0 && a[i] > key) {
            a[i + 1] = a[i];
            i--;
        }
        a[i + 1] = key;
    }
}

int main() {
    int a[] = {3, 5, 2, 6, 1, 4};
    int length = sizeof a / sizeof a[0];
    insertionSort(a, length);
    printArray(a, length);
    return 0;
}
