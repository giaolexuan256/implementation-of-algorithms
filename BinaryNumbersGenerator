#include <iostream>

using namespace std;

void printArray(int a[], int length) {
    for (int i = 0; i < length; i++) {
        cout << a[i];
    }
    cout << endl;
}

void setAllBackElementsToZero(int a[], int start, int length) {
    for (int i = start + 1; i < length; i++) {
        a[i] = 0;
    }
}

void generateBinaryNumbersOfLength(int length) {
    int a[length];
    for (int i = 0; i < length; i++) {
        a[i] = 0;
    }
    int i = length - 1;
    while (i >= 0) {
        printArray(a, length);
        while (a[i] != 0) {
            i--;
        }
        if (i >= 0) {
            a[i] = 1;
            setAllBackElementsToZero(a, i, length);
            i = length - 1;
        }

    }
}


int main() {
    int n;
    cin >> n;
    generateBinaryNumbersOfLength(n);
    return 0;
}
